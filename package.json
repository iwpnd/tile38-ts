{
    "name": "@tiermobility/tile38-ts",
    "version": "1.0.0-alpha.0",
    "description": "A Node.js Tile38 client written in TypeScript",
    "main": "dist/index.js",
    "files": [
        "dist"
    ],
    "typings": "dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/TierMobility/tile38-ts.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "author": "V. Priem <vinzent.priem@gmail.com>",
    "contributors": [
        "B. Ramser <b.ramser@posteo.net>"
    ],
    "license": "MIT",
    "keywords": [
        "tile38",
        "geofence",
        "geo-spatial",
        "database"
    ],
    "engines": {
        "node": ">=14.x",
        "yarn": "^1.22.x"
    },
    "private": false,
    "scripts": {
        "postinstall": "yarn husky install",
        "prepublishOnly": "pinst --disable && yarn run build",
        "postpublish": "pinst --enable",
        "build": "tsc",
        "build:watch": "tsc --watch",
        "clean": "rimraf {dist,tsconfig.tsbuildinfo,yarn-error.log,coverage}",
        "lint": "eslint --ext .ts ./src",
        "test": "jest --runInBand",
        "format": "prettier --write src/{*.ts,**/*.ts}",
        "u": "yarn upgrade-interactive --latest",
        "up": "docker-compose up tile38",
        "down": "docker-compose down"
    },
    "dependencies": {
        "@types/node": "17.0.43",
        "@vpriem/geojson": "1.0.1",
        "redis": "4.1.1"
    },
    "devDependencies": {
        "@commitlint/cli": "17.0.3",
        "@commitlint/config-conventional": "17.0.3",
        "@semantic-release/changelog": "6.0.1",
        "@semantic-release/git": "10.0.1",
        "@semantic-release/github": "8.0.4",
        "@semantic-release/npm": "9.0.1",
        "@semantic-release/release-notes-generator": "10.0.3",
        "@types/jest": "27.4.1",
        "@typescript-eslint/eslint-plugin": "5.30.0",
        "@typescript-eslint/parser": "5.30.0",
        "conventional-changelog-conventionalcommits": "5.0.0",
        "coveralls": "3.1.1",
        "devmoji": "2.3.0",
        "eslint": "8.18.0",
        "eslint-config-airbnb-base": "15.0.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-import-resolver-typescript": "3.1.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-prettier": "4.2.1",
        "husky": "8.0.1",
        "jest": "27.5.1",
        "lint-staged": "13.0.3",
        "pinst": "3.0.0",
        "prettier": "2.7.1",
        "rimraf": "3.0.2",
        "semantic-release": "19.0.3",
        "ts-jest": "27.1.4",
        "typescript": "4.7.4"
    },
    "lint-staged": {
        "*.ts": [
            "eslint --fix",
            "prettier --write"
        ]
    }
}
